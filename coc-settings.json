  "languageserver": {
    "java": {
      "enable": true,
      "command": "java-lang-server",
      "filetypes": ["java"]
    },
    "kotlin": {
      "enable": true,
      "command": "kotlin-language-server",
      "filetypes": ["kotlin"]
    },
    "dls": {
      "enable": false,
      "command": "/home/idanarye/.dub/packages/.bin/dls-latest/dls",
      "args": ["--stdio"],
      "filetypes": ["d"],
      "trace.server": "off",
      "rootPatterns": ["dub.json", "dub.sdl"]
    },
    "d": {
      "enable": false,
      "command": "dub",
      "args": ["run", "serve-d"],
      "filetypes": ["d"]
    }
  },
  "java.jdt.ls.home": "/usr/share/java/jdtls/",
  "java.errors.incompleteClasspath.severity": "ignore",
  "coc.source.omni.enable": true,
  "coc.source.omni.filetypes": ["python"],
  "json.schemas": [
    {
      "url": "file:///files/tests/jsonschema/1/schema.json",
      "fileMatch": [
        "/files/tests/jsonschema/1/*.json"
      ]
    },
    {
      "url": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json",
      "fileMatch": [
        ".vimspector.json"
      ]
    }
  ],
  "yaml.schemas": {
      "file:///files/tests/jsonschema/1/schema.json": "/files/tests/jsonschema/1/*.yaml",
      "file:///files/code/wekapp2/qa/schemas/iogen.schema.json": "/files/code/wekapp2/qa/iogen/configs/*.yaml",
      "file:///tmp/json-schema-test/schema.json": "/tmp/json-schema-test/data.yaml"
  },
  "git.signOffset": 999,
  "python.jediEnabled": false,
  "python.autoComplete.extraPaths": [
    "/files/code/easypy",
    "/files/code/talker",
    "/files/code/wekapp/deps/plumbum"
  ],
  "coc-actions.hideCursor": false,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "cd": ["cd"],
    "gS": ["reveal:sourceWindow"]
  },
  "explorer.file.autoReveal": false
}
